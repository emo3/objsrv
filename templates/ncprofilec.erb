<% @tools.each do |tool| -%>
alias start_<%= tool.procmd %>="$OMNIHOME/bin/nco_pa_start -server <%= tool.pa_name %> -user <%= tool.nc_act %> -password <%= tool.nc_pwd %> -process Primary<%= tool.prosrv %>"
alias stop_<%= tool.procmd %>="$OMNIHOME/bin/nco_pa_stop -server <%= tool.pa_name %> -user <%= tool.nc_act %> -password <%= tool.nc_pwd %> -process Primary<%= tool.prosrv %>"
<% end -%>
