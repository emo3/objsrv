<%= node['objsrv']['media_dir'] %><?xml version='1.0' encoding='UTF-8'?>
<agent-input clean='true' temporary='true'>
  <variables>
    <variable name='sharedLocation' value='<%= node['objsrv']['app_dir'] %>/IBMIMShared'/>
    <!-- Installation location for Netcool/OMNIbus Knowledge Library -->
    <variable name='installLocation.IBM_Netcool_Knowledge_Library' value='<%= node['objsrv']['app_dir'] %>/NcKL'/>
  </variables>
  <preference name='com.ibm.cic.common.core.preferences.eclipseCache' value='${sharedLocation}'/>
  <server>
    <repository location='file:<%= node['objsrv']['media_dir'] %>/nckl/'/>
  </server>
  <profile id='IBM Netcool Knowledge Library' installLocation='${installLocation.IBM_Netcool_Knowledge_Library}'>
    <data key='cic.selector.arch' value='x86_64'/>
  </profile>
  <install>
    <!-- Netcool/OMNIbus Knowledge Library 4.6.5 -->
    <offering profile='IBM Netcool Knowledge Library' id='com.ibm.tivoli.nckl' version='4.6.5' features='main.feature'/>
  </install>
</agent-input>
