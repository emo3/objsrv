# The gateway requires deletes be replicated, so that the cache can be
# purged of deleted alerts. Never suppress deletes here, use the 
# Gate.Jdbc.SuppressDeletes property instead
REPLICATE ALL FROM TABLE 'alerts.status'
	USING MAP 'StatusMap' ;

REPLICATE ALL FROM TABLE 'alerts.journal'
	USING MAP 'JournalMap';


